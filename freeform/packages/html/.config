; These are used to generate the DOCTYPE decl for HTMLDocument
html.version=4.01
html.type=transitional


; These two settings enable the parser cache
; Set this to 1 or 0 to enable or disable the parser cache
cache.parserCache=0

; Set this to WRITABLE by PHP directory. The directory MUST exist 
; before you enable this cache
cache.parserCachePath=/tmp/htmlparsercache/